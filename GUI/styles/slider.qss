QSlider::groove:horizontal {
    background: #888;
    height: 6px;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #eee;
    border: 1px solid #eee;
    width: 14px;
    height: 14px;
    margin: -4px 0;
    border-radius: 7px;
}

QSlider::sub-page:horizontal {
    background: #eee;
    border-radius: 3px;
}

QSlider::add-page:horizontal {
    background: #888;
    border-radius: 3px;
}
